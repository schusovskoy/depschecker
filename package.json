{
  "depschecker": {
    "ignoreUnmet": {
      "tsup": [
        "@swc/core",
        "@microsoft/api-extractor",
        "postcss"
      ]
    },
    "ignoreUnspecified": [
      "@eslint/js"
    ],
    "ignoreUnused": [
      "prettier"
    ]
