{
  "bin": "./cli.js",
  "dependencies": {
    "@babel/parser": "7.24.1",
    "@babel/traverse": "7.24.1",
    "@babel/types": "7.24.0",
    "@depschecker/project-graph": "workspace:*",
    "chalk": "4.1.2",
